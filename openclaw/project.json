{
  "name": "openclaw-app",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "openclaw/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "openclaw",
        "command": "corepack pnpm build"
      }
    },
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "openclaw",
        "command": "corepack pnpm dev"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "openclaw",
        "command": "corepack pnpm test"
      }
    },
    "n8n-vendor": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "openclaw",
        "command": "corepack pnpm n8n:vendor"
      }
    }
  }
}
