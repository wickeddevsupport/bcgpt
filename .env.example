BASECAMP_CLIENT_ID=
BASECAMP_CLIENT_SECRET=
BASECAMP_DEFAULT_ACCOUNT_ID=
APP_BASE_URL=https://bcgpt.wickedlab.io
PORT=3000

# n8n reverse proxy (same server, host-based routing)
N8N_PROXY_ENABLED=true
N8N_PROXY_HOST=automate.wickedlab.io
N8N_PROXY_TARGET=http://127.0.0.1:5678

# n8n API access (server-side)
N8N_INTERNAL_URL=http://127.0.0.1:5678
N8N_API_VERSION=1
# N8N_API_BASE_URL=http://127.0.0.1:5678/api/v1
N8N_TIMEOUT_MS=15000
N8N_RETRIES=2

# n8n runtime settings (when running n8n on the same host)
N8N_HOST=automate.wickedlab.io
N8N_PROTOCOL=https
N8N_PORT=5678
WEBHOOK_URL=https://automate.wickedlab.io/
N8N_PROXY_HOPS=1

# n8n custom nodes
# N8N_CUSTOM_EXTENSIONS=/opt/render/project/src/n8n/n8n-nodes-bcgpt
