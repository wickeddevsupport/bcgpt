{
  "name": "openclaw-frontend",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "frontend/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "frontend",
        "command": "npm run build"
      }
    },
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "frontend",
        "command": "npm run dev"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "frontend",
        "command": "npm run lint"
      }
    }
  }
}
